  "paths": {
    "/{channel}/repoItems": {
      "get": {
        "tags": [
          "Repoitems"
        ],
        "summary": "This endpoint allows you to retrieve all Repository Items you're authorized for and all subsequent sub-institutes. If the user identified by the Authorization Bearer Token header cannot be found, a 401 error is returned.",
        "parameters": [
          {
            "in": "path",
            "name": "channel",
            "description": "Select the desired channel to retrieve repoItems from",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "page[size]",
            "description": "Set the total amount of items for that page",
            "required": false,
            "schema": {
              "type": "integer"
            }
          },
          {
            "in": "query",
            "name": "page[number]",
            "description": "Set the page number",
            "required": false,
            "schema": {
              "type": "integer"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {
                  "oneOf": [
                    {
                      "type": "object",
                      "properties": {
                        "meta": {
                          "type": "object",
                          "properties": {
                            "totalCount": {
                              "type": "integer",
                              "example": 1242
                            }
                          }
                        },
                        "filters": {
                          "type": "array",
                          "items": {}
                        },
                        "links": {
                          "type": "object",
                          "properties": {
                            "first": {
                              "type": "string",
                              "example": "/api/jsonapi/channel/v1/edusources/repoItems?page[size]=2&page[number]=1"
                            },
                            "self": {
                              "type": "string",
                              "example": "/api/jsonapi/channel/v1/edusources/repoItems?page[size]=2&page[number]=1"
                            },
                            "next": {
                              "type": "string",
                              "example": "/api/jsonapi/channel/v1/edusources/repoItems?page[size]=2&page[number]=2"
                            },
                            "last": {
                              "type": "string",
                              "example": "/api/jsonapi/channel/v1/edusources/repoItems?page[size]=2&page[number]=1112"
                            }
                          }
                        },
                        "data": {
                          "$ref": "#/components/schemas/repoItemDataArray"
                        }
                      }
                    },
                    {
                      "$ref": "#/components/schemas/deletedRepoItems"
                    }
                  ]
                }
              }
            }
          },
          "400": {
            "description": "Bad request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/400"
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/401"
                }
              }
            }
          },
          "405": {
            "description": "Method not Allowed",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/405"
                }
              }
            }
          }
        }
      }
    },
