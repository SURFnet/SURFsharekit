{
    "name": "silverstripe/installer",
    "type": "silverstripe-recipe",
    "description": "The SilverStripe Framework Installer",
    "require": {
        "php": "^7.4",
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/recipe-cms": "4.6.1@stable",
        "zircote/swagger-php": "~3.0.3",
        "tuupola/base62": "^1.0",
        "ramsey/uuid": "^3.7",
        "ralouphie/mimey": "^2.1.0",
        "guzzlehttp/guzzle": "~6.0",
        "symbiote/silverstripe-gridfieldextensions": "^3",
        "silverstripe/s3": "^1.0",
        "nathancox/hasoneautocompletefield": "^2.11",
        "ext-json": "*",
        "jumbojett/openid-connect-php": "^0.9.10",
        "laravel/helpers": "^1.6",
        "bigfork/silverstripe-oauth-login": "^2.3",
        "firebase/php-jwt": "^6.10",
        "matomo/matomo-php-tracker": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "nikic/php-parser": "4.3.*"
    },
    "extra": {
        "resources-dir": "_resources",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ],
        "expose": [
            "app/src/javascript",
            "themes/surfsharekit/css",
            "themes/surfsharekit/images",
            "themes/surfsharekit/javascript"
        ]
    },
    "config": {
        "process-timeout": 600,
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/recipe-plugin": true,
            "silverstripe/vendor-plugin": true,
            "kylekatarnls/update-helper": false
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
